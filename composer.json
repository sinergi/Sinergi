{
  "name": "sinergi/users",
  "type": "library",
  "description": "Users, groups, authentication management",
  "keywords": ["users", "group", "auth", "authentication"],
  "homepage": "https://github.com/sinergi/users",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "psr/log": "^1.0",
    "container-interop/container-interop": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "doctrine/orm": "^2.5",
    "illuminate/database": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Sinergi\\Users\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sinergi\\Users\\Tests\\": ["test/tests"]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
